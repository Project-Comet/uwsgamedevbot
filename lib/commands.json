{
  "help": {
    "description": "Shows help for individual commands.",
    "alias": [
      "help",
      "man"
    ],
    "arguments": {
      "command": {
        "description": "Shows help for a specified command.",
        "optional": true
      }
    },
    "permissions": 100,
    "syntax": "[ command ]"
  },
  "version": {
    "description": "Shows the bot's current version.",
    "alias": [
      "version",
      "v"
    ],
    "arguments": {},
    "permissions": 100,
    "syntax": ""
  },
  "say": {
    "description": "Repeats a string.",
    "alias": [
      "say",
      "ping"
    ],
    "arguments": {
      "string": {
        "description": "The string that will be echoed.",
        "optional": false
      }
    },
    "permissions": 100,
    "syntax": "Pong!"
  },
  "quote": {
    "description": "Says something only Bjarne would say.",
    "alias": [
      "quote",
      "bjarnequote"
    ],
    "arguments": {},
    "permissions": 100,
    "syntax": ""
  },
  },
  "dice": {
    "description": "Rolls a die.",
    "alias": [
      "dice",
      "die"
    ],
    "arguments": {
      "faces": {
        "description": "The number of sides. 6 by default.",
        "optional": true
      }
    },
    "permissions": 100,
    "syntax": "42"
  },
  "math": {
    "description": "Does some math.",
    "alias": [
      "math"
    ],
    "arguments": {
      "number1": {
        "description": "The first number.",
        "optional": false
      },
      "operator": {
        "description": "The operator, either `+`, `-`, `*`, `/`, or `^`.",
        "optional": false
      },
      "number2": {
        "description": "The second number.",
        "optional": false
      }
    },
    "permissions": 100,
    "syntax": "[ 21 * 2 ] or [ 420 / 0 ]"
  }
}
